<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GeneratedDir>$(SolutionDir)build\$(Configuration)\$(ProjectName)\gen\</GeneratedDir>
    <GeneratedDirCommon>$(SolutionDir)build\$(Configuration)\common\gen\</GeneratedDirCommon>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="GeneratedDir">
      <Value>$(GeneratedDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GeneratedDirCommon">
      <Value>$(GeneratedDirCommon)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>